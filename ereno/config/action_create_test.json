{
  "action": "create_test",
  "description": "Create a test dataset and evaluate against existing training data",
  "input": {
    "benignDataPath": "target/benign_data/42_5%fault_benign_data.arff",
    "trainingDatasetPath": "target/training/training_dataset.arff",
    "verifyBenignData": true,
    "verifyTrainingData": true
  },
  "output": {
    "testDatasetDirectory": "target/testing",
    "testDatasetFilename": "test_dataset.arff",
    "evaluationOutputDirectory": "target/evaluation",
    "evaluationOutputFilename": "evaluation_results.json",
    "format": "arff"
  },
  "datasetStructure": {
    "messagesPerSegment": 500,
    "includeBenignSegment": true,
    "shuffleSegments": false
  },
  "evaluation": {
    "enabled": true,
    "classifiers": ["J48", "RandomForest", "NaiveBayes"],
    "outputMetrics": ["accuracy", "precision", "recall", "f1", "confusion_matrix"],
    "saveModel": false
  },
  "attackSegments": [
    {
      "name": "uc01_random_replay",
      "enabled": true,
      "attackConfig": "config/attacks/uc01_random_replay.json"
    },
    {
      "name": "uc02_inverse_replay",
      "enabled": true,
      "attackConfig": "config/attacks/uc02_inverse_replay.json"
    },
    {
      "name": "uc03_masquerade_fault",
      "enabled": true,
      "attackConfig": "config/attacks/uc03_masquerade_fault.json"
    },
    {
      "name": "uc04_masquerade_normal",
      "enabled": true,
      "attackConfig": "config/attacks/uc04_masquerade_normal.json"
    },
    {
      "name": "uc05_injection",
      "enabled": true,
      "attackConfig": "config/attacks/uc05_injection.json"
    },
    {
      "name": "uc06_high_stnum_injection",
      "enabled": true,
      "attackConfig": "config/attacks/uc06_high_stnum_injection.json"
    },
    {
      "name": "uc07_flooding",
      "enabled": true,
      "attackConfig": "config/attacks/uc07_flooding.json"
    },
    {
      "name": "uc08_grayhole",
      "enabled": true,
      "attackConfig": "config/attacks/uc08_grayhole.json"
    }
  ],
  "gooseFlow": {
    "numberOfMessages": 5000,
    "goID": "IntLockA",
    "ethSrc": "01:0c:cd:01:2f:80",
    "ethDst": "01:0c:cd:01:2f:81",
    "ethType": "0x88B8",
    "gooseAppid": "0x00003001",
    "TPID": "0x8100",
    "ndsCom": false,
    "test": false,
    "cbstatus": false
  },
  "devices": {
    "useCVariants": true
  }
}
